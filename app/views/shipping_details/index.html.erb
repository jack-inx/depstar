<h1>Listing Shipping Detail</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Email</th>
    <th>Payment method</th>
    <th>Paypal email</th>
    <th>Product</th>
    <th>Requires box</th>
    <th>Question response</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shipping_details.each do |shipping_detail| %>
  <tr>
    <td><%= shipping_detail.first_name %></td>
    <td><%= shipping_detail.last_name %></td>
    <td><%= shipping_detail.address1 %></td>
    <td><%= shipping_detail.address2 %></td>
    <td><%= shipping_detail.city %></td>
    <td><%= shipping_detail.state %></td>
    <td><%= shipping_detail.zip %></td>
    <td><%= shipping_detail.email %></td>
    <td><%= shipping_detail.payment_method_id %></td>
    <td><%= shipping_detail.paypal_email %></td>
    <td><%= shipping_detail.product_id %></td>
    <td><%= shipping_detail.requires_box %></td>
    <td><%= shipping_detail.question_response_id %></td>
    <td><%= link_to 'Show', shipping_detail %></td>
    <td><%= link_to 'Edit', edit_shipping_detail_path(shipping_detail) %></td>
    <td><%= link_to 'Destroy', shipping_detail, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

