<%#= render :file => "/admin/index"  %>
<h2>Carriers</h2>

<table class="hovertable">
  

<% @carriers.each do |carrier| %>
  <tr>
    <td><%= carrier.name %></td>
    <td><%= link_to 'Show', carrier %></td>
    <td><%= link_to 'Edit', edit_carrier_path(carrier) %></td>
    <td><%= link_to 'Destroy', carrier, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Create New Carrier', new_carrier_path %>
